local e=getgenv()e.Esp=e.Esp or{}local s,t,u,v=game:GetService("TweenService"),game:GetService("CoreGui"),Color3.fromRGB(0,255,255),Color3.fromRGB(255,50,100)local a=e.Active_Esp or{}e.Active_Esp=a;local m=setmetatable({},{__index=function(_,k)if k=="Set"then return function(o,m)local n=(m=="on"or m==true)if n then if a[o]then return end local f=Instance.new("Folder")f.Name="FX_"..o.Name;pcall(function()f.Parent=t end)local h,b=Instance.new("Highlight",f),Instance.new("SelectionBox",f)h.Adornee,b.Adornee=o,o;h.FillColor,h.OutlineColor,h.FillTransparency,h.DepthMode=u,u,1,0;b.LineThickness,b.Color3,b.SurfaceTransparency=0.05,u,1;s:Create(h,TweenInfo.new(0.5),{FillTransparency=0.8}):Play()s:Create(h,TweenInfo.new(1.5,3,0,-1,true),{OutlineColor=v}):Play()a[o]=f else if a[o]then a[o]:Destroy()a[o]=nil end end end elseif k=="GetActiveParts"then return function()local r={}for p,f in pairs(a)do if p and p.Parent then table.insert(r,p)else if f then f:Destroy()end a[p]=nil end end return r end end end})e.Esp=m;return m
